! URxvt {{{
urxvt*font           : xft: Monaco:size=10
urxvt.termName       : xterm-256color
!urxvt*keysym.Home    : \033[1~
!urxvt*keysym.End     : \033[4~
!urxvt.loginShell     : true
urxvt.transparent    : false
!urxvt.shading       : 10
urxvt.pointerColor   : #000000
urxvt.pointerColor2  : white
urxvt.borderless     : true
urxvt.colorMode      : on
urxvt.cursorBlink    : true
urxvt.internalBorder : 0
urxvt.externalBorder : 0


urxvt.fade: 0

urxvt.scrollBar:	false
urxvt.scrollTtyOuput: false
urxvt.cursorUnderline: true
! }}}
! Xft {{{
Xft.dpi:         96
Xft.antialias:   true
Xft.rgba:        rgb
Xft.autohint:    false
Xft.hinting:     true
Xft.hintstyle:   hintslight
Xft.lcdfilter:   lcdlight
! }}}}
! Colors {{{
*background: #2d2d2d
*foreground: #dcdccc
*cursorColor: #aaaaaa
*colorUL: #366060
*underlineColor: #dfaf8f
*color0: #3f3f3f
*color1: #cc9393
*color2: #7f9f7f
*color3: #d0bf8f
*color4: #6ca0a3
*color5: #dc8cc3
*color6: #93e0e3
*color7: #dcdccc
*color8: #000000
*color9: #dca3a3
*color10: #bfebbf
*color11: #f0dfaf
*color12: #8cd0d3
*color13: #dc8cc3
*color14: #93e0e3
*color15: #ffffff
! }}} 
