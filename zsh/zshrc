source $HOME/.dotfiles/zsh/grml-zsh-config.sh
source $HOME/.dotfiles/zsh/custom-functions.sh
export PATH=$HOME/.dotfiles/bin:$PATH
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

export FZF_DEFAULT_COMMAND="find $CWD -type f | grep -v node_modules | grep -v .git | grep -v vendor | sort"
